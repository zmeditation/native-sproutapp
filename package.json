{
  "name": "sprout",
  "version": "0.0.8",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean && npm i",
    "test": "ava --verbose",
    "test:watch": "ava --watch",
    "coverage": "nyc ava && nyc report --reporter=html && open coverage/index.html || xdg-open coverage/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "precommit": "npm run git-hook",
    "prepush": "npm run git-hook",
    "git-hook": "npm run lint -s"
  },
  "dependencies": {
    "analytics-react-native": "^1.2.0",
    "apisauce": "0.11.0",
    "apollo-cache-inmemory": "^1.1.4",
    "apollo-client": "^1.8.0",
    "apollo-link-context": "^1.0.3",
    "apollo-link-http": "^1.3.1",
    "art": "^0.10.1",
    "aws-sdk": "^2.82.0",
    "babel-plugin-transform-remove-console": "^6.8.4",
    "d3-scale": "^1.0.6",
    "format-json": "1.0.3",
    "graphql-client": "^1.1.0",
    "jasmine": "^2.6.0",
    "jwt-decode": "^2.2.0",
    "lodash": "4.17.4",
    "moment": "^2.18.1",
    "object-path": "^0.11.4",
    "object-path-immutable": "^0.5.1",
    "querystringify": "1.0.0",
    "ramda": "0.23.0",
    "react": "16.0.0-alpha.12",
    "react-intl": "^2.3.0",
    "react-native": "0.45.1",
    "react-native-aws-cognito-js": "0.0.5",
    "react-native-blur": "^3.1.2",
    "react-native-chart": "^1.0.8-beta",
    "react-native-circular-progress": "0.0.8",
    "react-native-code-push": "^5.1.3-beta",
    "react-native-communications": "^2.2.1",
    "react-native-config": "0.3.1",
    "react-native-datepicker": "^1.4.7",
    "react-native-device-info": "^0.10.2",
    "react-native-drawer": "2.3.0",
    "react-native-elements": "^0.11.2",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-highcharts": "^0.2.1",
    "react-native-image-picker": "^0.26.3",
    "react-native-keyboard-aware-scroll-view": "^0.3.0",
    "react-native-linear-gradient": "^2.0.0",
    "react-native-modalbox": "^1.4.0",
    "react-native-navigation": "^1.1.136",
    "react-native-pathjs-charts": "0.0.29",
    "react-native-pdf": "^2.0.6",
    "react-native-progress": "^3.3.0",
    "react-native-router-flux": "3.38.0",
    "react-native-snap-carousel": "^2.4.0",
    "react-native-swiper": "^1.5.4",
    "react-native-tab-navigator": "^0.3.4",
    "react-native-touch-id": "^3.1.0",
    "react-native-vector-icons": "^4.0.1",
    "react-navigation": "^1.0.0-beta.9",
    "react-redux": "5.0.4",
    "react-swipeable-views-native": "^0.12.8",
    "redux": "3.6.0",
    "redux-form": "^6.6.3",
    "redux-persist": "4.6.0",
    "redux-saga": "^0.14.6",
    "reduxsauce": "0.4.1",
    "reselect": "^3.0.1",
    "seamless-immutable": "7.1.2",
    "styled-components": "^1.4.6",
    "validate.js": "^0.11.1",
    "victory-native": "^0.12.0"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "enzyme": "^2.6.0",
    "husky": "^0.13.1",
    "ignite-ir-boilerplate-2016": "^0.2.2",
    "ignite-vector-icons": "^0.2.1",
    "jest": "^19.0.2",
    "mockery": "^2.0.0",
    "nyc": "^10.1.2",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.4.0",
    "react-native-communications": "^2.2.1",
    "react-native-mock": "^0.3.1",
    "reactotron-apisauce": "^1.7.0",
    "reactotron-react-native": "^1.7.0",
    "reactotron-redux": "^1.7.0",
    "reactotron-redux-saga": "^1.7.0",
    "snazzy": "^6.0.0",
    "standard": "8.6.0"
  },
  "ava": {
    "files": [
      "Tests/**/*.js",
      "!Tests/Setup.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "react-native-mock/mock",
      "./Tests/Setup"
    ],
    "babel": "inherit"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "if [ -d 'ignite-base' ]; then cd ignite-base; fi; npm run lint"
    }
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
}
